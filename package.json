{
  "name": "junta-tribo",
  "version": "1.0.0",
  "private": true,
  "description": "Multi-user travel app monorepo",
  "scripts": {
    "build": "npm run build:shared && npm run build:api && npm run build:web",
    "build:shared": "cd packages/shared && npm run build",
    "build:api": "cd apps/api && nest build && tsc --project tsconfig.build-cli.json",
    "build:web": "cd apps/web && next build",
    "build:prod": "npm run clean && npm run build",
    "dev": "concurrently \"npm run dev:api\" \"npm run dev:web\"",
    "dev:api": "cd apps/api && nest start --watch",
    "dev:web": "cd apps/web && next dev -p 3003",
    "start:prod:api": "cd apps/api && node dist/src/main.js",
    "start:prod:web": "cd apps/web && next start -p ${PORT:-3003}",
    "lint": "npm run lint:api && npm run lint:web",
    "lint:api": "cd apps/api && eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:web": "cd apps/web && next lint",
    "test": "jest",
    "test:api": "jest --selectProjects api",
    "test:web": "jest --selectProjects web",
    "test:watch": "jest --watch",
    "test:watch:api": "jest --selectProjects api --watch",
    "test:watch:web": "jest --selectProjects web --watch",
    "test:coverage": "jest --coverage",
    "type-check": "npm run type-check:api && npm run type-check:web",
    "type-check:api": "cd apps/api && tsc --noEmit",
    "type-check:web": "cd apps/web && tsc --noEmit",
    "clean": "rm -rf packages/shared/dist apps/api/dist apps/web/.next",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "deploy": "npm run build:prod && pm2 restart all",
    "pm2:start": "pm2 start pm2.json",
    "pm2:stop": "pm2 stop all",
    "pm2:restart": "pm2 restart all",
    "pm2:stop:backend": "pm2 stop juntatribo-api",
    "pm2:stop:frontend": "pm2 stop juntatribo-web",
    "pm2:restart:backend": "pm2 restart juntatribo-api",
    "pm2:restart:frontend": "pm2 restart juntatribo-web",
    "pm2:list": "pm2 list",
    "migration:generate": "cd apps/api && typeorm-ts-node-commonjs migration:generate",
    "migration:run": "cd apps/api && typeorm-ts-node-commonjs migration:run",
    "migration:run:prod": "cd apps/api && npx typeorm migration:run -d dist/typeorm-cli.config",
    "migration:revert": "cd apps/api && typeorm-ts-node-commonjs migration:revert"
  },
